        <arguments>
          <argument>
            <kind>1</kind>
            <string>///przewijanie miesiecy &amp; wysowanie panelu z lewej
if device_mouse_check_button_pressed(0,mb_left)
{
    {
    x_mouse_prev=device_mouse_x(0)
    y_mouse_prev=device_mouse_y(0)
    
    }
}else if device_mouse_check_button_released(0,mb_left)
{
    if interface=1
    {
    interface=0
    }
    if device_mouse_x(0)&lt;global.szer and device_mouse_y(0)&lt;Y_MARGIN and !instance_exists(objInterface3) and !instance_exists(objInterface5)//wlaczanie paelu z przycisku
    {
        interface=3
        slide_x=1
    }
} else if device_mouse_check_button(0,mb_left)
{
    if interface=0
    {
        // przewijanie miesiecy
        if point_distance(0,y_mouse_prev,0,device_mouse_y(0))&gt;global.szer/4
        {
            interface=1
            if !instance_exists(draw_msc_id[0])
            {
                if miesiac_teraz=0 and rok_teraz=0
                    draw_msc_id[0]=scrCreate_draw_msc(miesiac_teraz,rok_teraz)
                else
                    draw_msc_id[0]=scrCreate_draw_msc(miesiac_teraz-1,rok_teraz)
            }
            if !instance_exists(draw_msc_id[2])
                draw_msc_id[2]=scrCreate_draw_msc(miesiac_teraz+1,rok_teraz)
            y_mouse_prev=device_mouse_y(0)
            global.slide_y=0
        }
        // aktywowanie panelu z lewej - wysuwanie
        if point_distance(x_mouse_prev,0,device_mouse_x(0),0)&gt;global.szer/4 and x_mouse_prev&lt;global.szer/2 
        {
            interface=3
            slide_x=device_mouse_x(0)/(global.szer*7)
        }
    }
    if interface=1
    {
        if miesiac_teraz=0 and rok_teraz=0 //blokada na styczniu 2020
            global.slide_y=max(-1,min((device_mouse_y(0)-y_mouse_prev)/global.wys/2,0))*-1
        else
            global.slide_y=max(-1,min((device_mouse_y(0)-y_mouse_prev)/global.wys/2,1))*-1
    ///slide
        if global.slide_y&gt;0 //msc+
        {
/*---------------- DODAWANIE MIESIECY ----------------*/
            if global.slide_y&gt;=1
            {
                global.slide_y-=1
                y_mouse_prev-=global.wys*2
                if miesiac_teraz&lt;11
                    miesiac_teraz+=1
                else
                {
                    miesiac_teraz=0
                    rok_teraz+=1
                }
                if instance_exists(draw_msc_id[0]){
                    with(draw_msc_id[0]){instance_destroy()}}
                draw_msc_id[0]=draw_msc_id[1]
                draw_msc_id[1]=draw_msc_id[2]
                draw_msc_id[2]=scrCreate_draw_msc(miesiac_teraz+1, rok_teraz)
            }
        }
        else //msc-
        {
/*---------------- ODEJMOWANIE MIESIECY ----------------*/
            if global.slide_y&lt;=-1
            {
                global.slide_y+=1
                y_mouse_prev+=global.wys*2
                if miesiac_teraz&gt;0
                    miesiac_teraz-=1
                else
                {
                    miesiac_teraz=11
                    rok_teraz-=1
                }
                if instance_exists(draw_msc_id[2]){
                    with(draw_msc_id[2]){instance_destroy()}}
                draw_msc_id[2]=draw_msc_id[1]
                draw_msc_id[1]=draw_msc_id[0]
                if miesiac_teraz=0 and rok_teraz=0
                    draw_msc_id[0]=scrCreate_draw_msc(miesiac_teraz,rok_teraz)
                else
                    draw_msc_id[0]=scrCreate_draw_msc(miesiac_teraz-1,rok_teraz)
            }
        }
    }
/*--------------------------------------------------------*/
/*---------------- Panel wysuwany z lewej ----------------*/
/*--------------------------------------------------------*/
    else if interface=3
    {
        //sterowanie zasuwania panelu
        if slide_x!=0 and slide_x!=1
            slide_x=device_mouse_x(0)/(global.szer*7)
        //aktywowanie panelu z lewej - zamykanie
        if point_distance(x_mouse_prev,0,device_mouse_x(0),0)&gt;global.szer/4 and x_mouse_prev&gt;global.szer*6+global.szer/2 and !instance_exists(objInterface3)
        {
            slide_x=device_mouse_x(0)/(global.szer*7)
        }
    }
}else//wyrownanie miesiecy po puszczeniu klawisza
{
    if global.slide_y!=0
    {
        if global.slide_y&gt;0 //miesiac +
        {
            if global.slide_y&gt;0.5
            {
                global.slide_y=min(global.slide_y*1.07,1)
                if global.slide_y=1
                {
                    global.slide_y=0
                    if miesiac_teraz&lt;11
                        miesiac_teraz+=1
                    else
                    {
                        miesiac_teraz=0
                        rok_teraz+=1
                    }
                    if instance_exists(draw_msc_id[0]){
                        with(draw_msc_id[0]){instance_destroy()}}
                    draw_msc_id[0]=draw_msc_id[1]
                    draw_msc_id[1]=draw_msc_id[2]
                    draw_msc_id[2]=scrCreate_draw_msc(miesiac_teraz+1, rok_teraz)
                }
            }else 
            {
                global.slide_y=max(global.slide_y*0.65,0)
                if global.slide_y&lt;0.01
                {   
                    global.slide_y=0
                    with(draw_msc_id[0]){instance_destroy()}
                    with(draw_msc_id[2]){instance_destroy()}
                }
            }
        }
        else //miesiac -
        {
            if global.slide_y&lt;-0.5
            {
                global.slide_y=max(global.slide_y*1.07,-1)
                if global.slide_y=-1
                {
                    global.slide_y=0
                    if miesiac_teraz&gt;0
                        miesiac_teraz-=1
                    else
                    {
                        miesiac_teraz=11
                        rok_teraz-=1
                    }
                    if instance_exists(draw_msc_id[2]){
                        with(draw_msc_id[2]){instance_destroy()}}
                    draw_msc_id[2]=draw_msc_id[1]
                    draw_msc_id[1]=draw_msc_id[0]
                    if miesiac_teraz=0 and rok_teraz=0
                        draw_msc_id[0]=scrCreate_draw_msc(miesiac_teraz,rok_teraz)
                    else
                        draw_msc_id[0]=scrCreate_draw_msc(miesiac_teraz-1,rok_teraz)
                }
            }else
            {
                global.slide_y=min(global.slide_y*0.65,0)
                if global.slide_y&gt;-0.01
                {
                    global.slide_y=0
                    with(draw_msc_id[0]){instance_destroy()}
                    with(draw_msc_id[2]){instance_destroy()}
                }
            }
        }
    }
    else
    {
        if instance_exists(draw_msc_id[2]){
            with(draw_msc_id[2]){instance_destroy()}}
        if instance_exists(draw_msc_id[0]){
            with(draw_msc_id[0]){instance_destroy()}}
    }
    //panel z lewej
    if interface=3
    {
    if slide_x&gt;0.5 and slide_x!=1 //wysuwnie
    {
        slide_x=min(slide_x*1.08,1)
    }
    if slide_x&lt;=0.5 and slide_x!=0 //chowanie
    {
        slide_x=max(slide_x-max(0.05,(slide_x-(slide_x*0.90))),0)
    }
    if slide_x=0 and !instance_exists(objInterface3)  interface=0
    }
    
}
if global.slide_y&gt;0
{
/*---------------- RUCH MIESIACAMI ----------------*/
            if instance_exists(draw_msc_id[0]){
                with(draw_msc_id[0]){
                    image_alpha=1
                    y=-abs((1+global.slide_y)*global.wys*7);
                    depth=1}}
            if instance_exists(draw_msc_id[1]){
                with(draw_msc_id[1]){
                    image_alpha=1
                    y=-abs(global.slide_y*global.wys*7);
                    depth=2}}
            if instance_exists(draw_msc_id[2]){
                with(draw_msc_id[2]){
                    image_alpha=abs(global.slide_y)
                    y=0
                    depth=3}}
}else
{
/*---------------- RUCH MIESIACAMI ----------------*/
            if instance_exists(draw_msc_id[0]){
                with(draw_msc_id[0]){
                    image_alpha=1
                    y=-global.wys*7+abs(global.slide_y)*global.wys*7;
                    depth=1}}
            if instance_exists(draw_msc_id[1]){
                with(draw_msc_id[1]){
                    image_alpha=1-abs(global.slide_y)
                    y=0
                    depth=2}}
            if instance_exists(draw_msc_id[2]){
                with(draw_msc_id[2]){
                    image_alpha=-abs(global.slide_y)
                    y=0
                    depth=3}}
}
