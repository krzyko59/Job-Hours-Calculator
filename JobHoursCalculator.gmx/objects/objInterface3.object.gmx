            <string>///czytanie wiadomosci
var msg_id = ds_map_find_value(async_load, "id");
switch(msg_id)
{
case msg_nazwa:
    if ds_map_find_value(async_load, "status")
    {
        var _nazwa;
        _nazwa=ds_map_find_value(async_load, "result")
        if _nazwa!=""
        {
        nazwa=_nazwa
        objSystem.dane_stawka[id_stawki, STAWKA_NAZWA]=nazwa
            ini_open("stawki.ini")
            ini_write_string(id_stawki,STAWKA_NAZWA,nazwa)
            ini_close()
        }else
        show_message_async("!!!UWAGA!!!#NAZWA NIE MOŻE BYĆ PUSTA")  
    }
break;
case msg_stawka:
    if ds_map_find_value(async_load, "status")
    {
        var _stawka;
        _stawka=ds_map_find_value(async_load, "result")
        if _stawka&lt;0
        {
            show_message_async("!!!UWAGA!!!#STAWKA GODZINOWA NIE MOŻE BYĆ UJEMNA")
        }else
        {
        stawka=_stawka
        objSystem.dane_stawka[id_stawki, STAWKA_STAWKA]=stawka
            ini_open("stawki.ini")
            ini_write_string(id_stawki,STAWKA_NAZWA,nazwa)
            ini_write_real(id_stawki,STAWKA_STAWKA,stawka)
            ini_close()
        }
    }
break;
}
</string>
